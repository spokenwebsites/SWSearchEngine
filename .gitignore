!/tmp/pids/.keep

# Ignore uploaded files in development.
blacklight_front/storage/*
!/storage/.keep
blacklight_front/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

blacklight_front/public/assets

# Ignore master key for decrypting credentials and more.
blacklight_front/config/master.key
